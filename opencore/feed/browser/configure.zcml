<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser"
           >

  <browser:page
     for="opencore.feed.interfaces.ICanFeed"
     name="rss"
     class=".view.RssView"
     template="rss.pt"
     permission="zope2.View"
     />

  <browser:page
     for="opencore.feed.interfaces.ICanFeed"
     name="atom"
     class=".view.RssView"
     template="atom.pt"
     permission="zope2.View"
     />

</configure>
